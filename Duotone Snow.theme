            "syntax highlight": [
              {
                "comment": "#737577"
              },
              {
                "constant": "#782b87"
              },
              {
                "number": "#762727"
              },
              {
                "string": "#ad435b"
              },
              {
                "entity": "#0b3587"
              },
              {
                "keyword": "#0f60da"
              },
              {
                "function": "#782b87"
              },
              {
                "variable": "#0b3587"
              }
            ]
          }
        ]
      },
      {
        "task": [
          {
            "background color": "$base.background color"
          },
          {
            "border color": "#9EB0CE"
          },
          {
            "check color": "#9EB0CE"
          }
        ]
      },
      {
        "tag": [
          {
            "background color": "#B1BDCD"
          },
          {
            "text color": "#FFFFFF"
          },
          {
            "marker color": "#436AA3"
          }
        ]
      },
      {
        "highlighter": [
          {
            "background color": "$base.highlight color"
          },
          {
            "text color": "$base.text color"
          }
        ]
      },
      {
        "separator": [
          {
            "border color": "#9EB0CE"
          }
        ]
      },
      {
