          {
            "font": "RobotoMono-Regular"
          },
          {
            "text size multiplier": 0.91
          },
          {
            "syntax highlight": [
              {
                "comment": "#a3a6a9"
              },
              {
                "constant": "#C33ED4"
              },
              {
                "number": "#D84848"
              },
              {
                "string": "#e48952"
              },
              {
                "entity": "#557de4"
              },
              {
                "keyword": "#e38e13"
              },
              {
                "function": "#C141D1"
              },
              {
                "variable": "#557de4"
              }
            ]
          }
        ]
      },
      {
        "task": [
          {
            "background color": "$base.background color"
          },
          {
            "border color": "$base.text secondary color"
          },
          {
            "check color": "$base.text color"
          }
        ]
      },
      {
        "tag": [
          {
            "background color": "#323333"
          },
          {
            "text color": "#DFDFDF"
          },
          {
            "marker color": "#FCCE22"
          }
        ]
      },
      {
        "highlighter": [
          {
            "background color": "$base.highlight color"
          },
          {
            "text color": "$base.text color"
          }
        ]
      },
      {
        "separator": [
          {
            "border color": "#B06E12"
          }
        ]
      },
      {
        "table": [
          {
            "border color": "$base.stroke color"
          },
          {
            "cell background color": "$base.background color"
          },
          {
            "cell alternate background color": "$base.background secondary color"
          }
        ]
      },
      {
        "toolbar" : [
          {
            "icon color" : "$base.text secondary color"
          },
          {
            "hover color" : "$base.background tertiary color"
          }
       ]
      }
    ]
  }
]
