[
  {
    "base": [
      {
        "text color": "#fdfae0"
      },
      {
        "text secondary color": "#949494"
      },
      {
        "text tertiary color": "#cccccc"
      },
      {
        "background color": "#18181e"
      },
      {
        "background secondary color": "#1F1F26"
      },
      {
        "background tertiary color": "#3A3A44"
      },
      {
        "stroke color": "#32323E"
      },
      {
        "accent color": "#d15f26"
      },
      {
        "search primary color": "#6fbd75"
      },
      {
        "search secondary color": "$base.selection color"
      },
      {
        "highlight color": "#0B5652"
      },
      {
        "selection color": "#565669"
      }
    ]
  },
  {
    "notes" : [
    {
      "title color": "$editor.headers.text color"
    },
    {
      "subtitle color": "$base.text secondary color"
    },
    {
      "placeholder color": "$base.text secondary color"
    },
    {
      "date color": "$base.text secondary color"
    },
    {
      "pin color": "#90CCAC"
    },
    {
      "separator color": "#32323E"
    },
    {
      "search background color": "$base.search primary color"
    },
    {
      "attachment background color": "$base.background secondary color"
    },
    {
      "encrypted token color": "$base.background tertiary color"
    },
    {
      "selection background color": "#24242C"
    },
    {
      "ribbon color": "$base.accent color"
    },
    {
      "toolbar" : [
        {
          "background color" : "$base.background color"
        },
        {
          "text color" : "$base.text color"
        },
        {
          "icon color" : "#C192AF"
        },
        {
          "hover color" : "$base.background secondary color"
        }
      ]
    }
   ]
  },
  {
    "sidebar": [
      {
        "text color": "#fdfae0"
      },
      {
        "text secondary color": "#070709"
      },
      {
        "background color": "#22232d"
      },
      {
        "background secondary color": "#9f6399"
      },
      {
        "stroke color": "#32323E"
      },
      {
        "accent color": "#c36fb3"
      },
      {
        "icon color": "#70bf75"
      },
      {
        "selected icon color": "#17181e"
      },
      {
        "toolbar" : [
          {
            "background color" : "$sidebar.background color"
          },
          {
            "icon color" : "$sidebar.icon color"
          },
          {
            "hover color" : "#191A20"
          }
       ]
      }
    ]
  },
  {
    "placeholder": [
      {
        "background color": "$base.background color"
      },
      {
        "background stroke color": "#49462c"
      },
      {
        "shadow color": "#18171e"
      },
      {
        "stroke color": "#6e683c"
      }
    ]
  },
  {
    "debug": [
      {
        "show grid": 0
      },
      {
        "show editing range": 0
      }
    ]
  },
  {
    "editor": [
      {
        "background color": "$base.background color"
      },
      {
        "text color": "$base.text color"
      },
      {
        "text light color": "$base.text secondary color"
      },
      {
        "accent color": "$base.accent color"
      },
      {
        "cursor color": "$base.accent color"
      },
      {
        "link color": "#7bceab"
      },
      {
        "list marker color": "$base.accent color"
      },
      {
        "marker color": "#458e89"
      },
      {
        "selection color": "$base.selection color"
      },
      {
        "selection inactive color": "#25252D"
      },
      {
        "text font": "BearSansUI-Regular"
      },
      {
        "text size": 15
      },
      {
        "line height multiplier": 1.5
      },
      {
        "headers": [
          {
            "text color": "#fbef89"
          },
          {
            "font": "BearSansUIHeadline-Medium"
          },
          {
            "modular scale": 1.125
          },
          {
            "line height multiplier": 1.4
          },
          {
            "add top bottom padding": 1
          },
          {
            "padding top multiplier": 0.5
          },
          {
            "padding bottom multiplier": 0.3
          }
        ]
      },
      {
        "code": [
          {
            "text color": "$base.text color"
          },
          {
            "border color": "$base.text tertiary color"
          },
          {
            "background color": "$base.background secondary color"
          },
          {
            "font": "RobotoMono-Regular"
          },
          {
            "text size multiplier": 0.91
          },
          {
            "syntax highlight": [
              {
                "comment": "#787d92"
              },
              {
                "constant": "#8eccac"
              },
              {
                "number": "#c092af"
              },
              {
                "string": "#ff8b48"
              },
              {
                "entity": "#ca90b1"
              },
              {
                "keyword": "#7bceab"
              },
              {
                "function": "#d8a189"
              },
              {
                "variable": "#ca90b1"
              }
            ]
          }
        ]
      },
      {
        "task": [
          {
            "background color": "$base.background color"
          },
          {
            "border color": "#d15f25"
          },
          {
            "check color": "#d15f25"
          }
        ]
      },
      {
        "tag": [
          {
            "background color": "#448d89"
          },
          {
            "text color": "#efedd3"
          },
          {
            "marker color": "#FBEF89"
          }
        ]
      },
      {
        "highlighter": [
          {
            "background color": "$base.highlight color"
          },
          {
            "text color": "$base.text color"
          }
        ]
      },
      {
        "separator": [
          {
            "border color": "#c36fb3"
          }
        ]
      },
      {
        "table": [
          {
            "border color": "$base.stroke color"
          },
          {
            "cell background color": "$base.background color"
          },
          {
            "cell alternate background color": "$base.background secondary color"
          }
        ]
      },
      {
        "toolbar" : [
          {
            "icon color" : "$notes.toolbar.icon color"
          },
          {
            "hover color" : "$base.background secondary color"
          }
       ]
      }
    ]
  }
]
