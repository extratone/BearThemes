          },
          {
            "text size multiplier": 0.91
          },
          {
            "syntax highlight": [
              {
                "comment": "#a9b3d2"
              },
              {
                "constant": "#94bece"
              },
              {
                "number": "#a7bd90"
              },
              {
                "string": "#e39e86"
              },
              {
                "entity": "#B798B5"
              },
              {
                "keyword": "#94bece"
              },
              {
                "function": "#d8a189"
              },
              {
                "variable": "#B698B4"
              }
            ]
          }
        ]
      },
      {
        "task": [
          {
            "background color": "$base.background color"
          },
          {
            "border color": "#6c7a92"
          },
          {
            "check color": "#6c7a92"
          }
        ]
      },
      {
        "tag": [
          {
            "background color": "#4e5568"
          },
          {
            "text color": "#d9dde7"
          },
          {
            "marker color": "#A7BD90"
          }
        ]
      },
      {
        "highlighter": [
          {
            "background color": "$base.highlight color"
          },
          {
            "text color": "$base.text color"
          }
        ]
      },
      {
        "separator": [
          {
            "border color": "#4e5568"
          }
        ]
      },
      {
        "table": [
          {
            "border color": "$base.stroke color"
          },
          {
            "cell background color": "$base.background color"
          },
          {
            "cell alternate background color": "$base.background secondary color"
          }
        ]
      },
      {
        "toolbar" : [
          {
            "icon color" : "$notes.toolbar.icon color"
          },
          {
            "hover color" : "$base.background secondary color"
          }
       ]
      }
    ]
  }
]
